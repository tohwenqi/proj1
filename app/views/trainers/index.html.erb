<div class="container">
  <div class="row">
    <h3>Trainers</h3>
    <div class="column">
      <table class="table">
        <tbody>
          <% @trainers.each do |trainer| %>
            <tr>
              <%= render trainer %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
    <%= button_to "Create new Pokemon!", new_pokemon_path, :class => "button large", :method => :get %> <br>
  </div>
</div>

